{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Start Server",
            "runtimeVersion": "14.20.0",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceRoot}/dist/app.js"
        },
        {
            "name": "admin-ui",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:4200/",
            "webRoot": "${workspaceRoot}/src"
        }
    ]
}